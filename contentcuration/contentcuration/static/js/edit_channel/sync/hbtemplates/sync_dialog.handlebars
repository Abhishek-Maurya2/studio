<div id="sync_bottom_container">
	<button class="action-text" data-dismiss="modal">{{formatMessage (intlGet 'messages.cancel')}}</button>
	<button disabled class="action-button pull-right disabled" id="sync_content_button">{{formatMessage (intlGet 'messages.sync')}}</button>
	<span id="sync_status" class="pull-right truncate"></span>
</div>