<div class="sync_section_wrapper">
	<div class="col-sm-4 sync_section">
		<div id="changed_list_area" class="sync_container"><div class="empty_default">{{formatMessage (intlGet 'messages.check_changes')}}</div></div>
	</div>
	<div class="col-sm-8 sync_section">
		<div id="sync_preview_section" class="sync_container"></div>
	</div>
</div>
