<div class="input-tab-control tab_item" data-next="#license_close_button"></div>
<div class="modal fade" id="license_modal"  tabindex="-1">
<div class="modal-dialog">
    <div class="modal-content">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close" id="close_license" class="tab_item"><span aria-hidden="true">&times;</span></button>
            <h4 class="modal-title text-center">{{license.license_name}}</h4>
        </div>
        <div class="modal-body">
        	<div  class="modal-body-content">
            	<p>{{license.license_description}}</p>
            </div>
            <div class="container-fluid">
    			{{#if license.license_url}}<a target="_blank" href="{{license.license_url}}" class="action-text tab_item" id="learn_more_button">LEARN MORE</a>{{/if}}
    			<button class="action-button pull-right tab_item" data-dismiss="modal" id="license_close_button">CLOSE</button>
            </div>
        </div>
    </div>
</div>
</div>
<div class="input-tab-control tab_item" data-next="#close_license"></div>